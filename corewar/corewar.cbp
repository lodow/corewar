<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="corewar" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="all">
				<Option output="./corewar" prefix_auto="1" extension_auto="1" />
				<Option object_output="./" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile $target" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile $target" />
					<SilentBuild command="$make -s -f $makefile $target" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="Makefile" />
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="vmstats.h" />
		<Unit filename="../misc/get_file.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../misc/get_file.h" />
		<Unit filename="../misc/get_next_line.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../misc/get_next_line.h" />
		<Unit filename="../misc/liste.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../misc/liste.h" />
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
